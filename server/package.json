{
    "name": "test_server",
    "version": "0.0.1",
    "private": false,
    "bin": {
        "$": "./dist/bin/app.js"
    },
    "main": "./dist/app",
    "scripts": {
        "test": "npm run build && cd dist && node app",
        "watch": "tsc --watch --noEmit --noErrorTruncation --pretty",
        "build": "npm i && tsc && node ./tools/copy",
        "build-deps": "npm run build-route && npm i ../route",
        "build-route": "cd ../route && npm i && npm run build"
    },
    "dependencies": {
        "@types/bluebird": "^3.5.19",
        "@types/node": "^12.7.12",
        "bluebird": "^3.5.1",
        "flatbuffers": "file:../flatbuffers-js",
        "pinus": "1.6.3",
        "reflect-metadata": "^0.1.10",
        "route": "file:../route",
        "source-map-support": "^0.5.0"
    },
    "devDependencies": {
        "tslint": "5.20.1",
        "typescript": "^4.3.5"
    }
}
